<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rezakhan.android.salesapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rezakhan.android.salesapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rezakhan.android.salesapp.RegAct" />
        <activity android:name="com.rezakhan.android.salesapp.HomeAct" />
        <activity android:name="com.rezakhan.android.salesapp.ItemAct" />
        <activity android:name="com.rezakhan.android.salesapp.OrderAct" />
        <activity android:name="com.rezakhan.android.salesapp.TotalAct" />
        <activity android:name="com.rezakhan.android.salesapp.ConfirmAct"></activity>
    </application>

</manifest>